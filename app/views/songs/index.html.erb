<div class="song-list">
<% @songs.each do |song| %>
 <div class="list-group-item">
 <%= link_to song_path(song.id) do %>
 <%end%>
<ul class="song">

    <li><%=link_to(song.name,song_path(song.id))%>
    <% song.albums.each do |album| %>
      <%= album.name %>
    <%end%>
    </li>
    <li><%= link_to('NEW SONG', new_song_path)%></li>
    <li><%=link_to('EDIT', edit_song_path(song.id))%></li>
    <li><%=link_to('DELETE',song_path(song.id), method: :delete) %></li>


      </ul>
    </div>
  <%end%>
</div>